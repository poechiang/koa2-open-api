{
  "compilerOptions": {
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    
    "baseUrl": "./",
    "moduleResolution": "node",

    "target": "ES6",
    "module": "CommonJS",
    
    
    "esModuleInterop": true,
    "sourceMap": true,


    "outDir": "./dist",
    "types":[
      "node"
    ],
    "lib": [
      "ES6"
    ],
    "paths": {
      "~/*":["./src/*"],
      "@app/*":["./src/app/*"],
      "@config/*":["./src/config/*"],
      "@R/*":["./src/routes/*"],
      "@Lib/*":["./src/lib/*"],
      "@Mid/*":["./src/midwares/*"],
      "@M/*":["./src/models/*"],
      "@svr/*":["./src/service/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["./src/**/*.ts"]
}